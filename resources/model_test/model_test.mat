#type mat

block properties {
    vec4 color;
}
section frag {
    out vec4 diffuse() {
    GLSL
        return color;
    END
    }
    out vec3 f_position(in vec3 f_position) {
    GLSL
        return f_position;
    END
    }
    out vec3 f_normal(in vec3 f_normal) {
    GLSL
        return f_normal;
    END
    }
}
